semmodel_w1 <- '

cholesbio =~ bld_hdlchol_w1 + bld_triglyc_w1
choles =~ cholesbio + hichol_w1

dbp =~ dbp1sit_w1 + dbp2sit_w1 + dbp3sit_w1 
sbp =~ sbp1sit_w1 + sbp2sit_w1 + sbp3sit_w1 
bp =~ dbp + sbp

cardio =~ cvdhist_w1

diab =~ bld_hba1c_w1 + diab_w1

choles ~ lonely_w1
choles ~ sex
choles ~ hmsonum_w1
choles ~ hiqual_w1
choles ~ phyactiv_w1
choles ~ livealn_w1
choles ~ hadsd_w1
choles ~ hadsa_w1



bp ~ lonely_w1
bp ~ sex
bp ~ hmsonum_w1
bp ~ hiqual_w1
bp ~ phyactiv_w1
bp ~ livealn_w1
bp ~ hadsd_w1
bp ~ hadsa_w1

cardio ~ lonely_w1
cardio ~ sex
cardio ~ hmsonum_w1
cardio ~ hiqual_w1
cardio ~ phyactiv_w1
cardio ~ livealn_w1
cardio ~ hadsd_w1
cardio ~ hadsa_w1


diab ~ lonely_w1
diab ~ sex
diab ~ hmsonum_w1
diab ~ hiqual_w1
diab ~ phyactiv_w1
diab ~ livealn_w1
diab ~ hadsd_w1
diab ~ hadsa_w1







#biomarkers covariances
bld_triglyc_w1 ~~ bld_hba1c_w1
bld_triglyc_w1 ~~ dbp
bld_hdlchol_w1 ~~ bld_hba1c_w1
bld_hdlchol_w1 ~~ sbp
bld_hba1c_w1 ~~ dbp
bld_hba1c_w1 ~~ sbp

bld_triglyc_w1  ~~ sbp
bld_hdlchol_w1 ~~ dbp



#selfreport covariances
hichol_w1 ~~ diab_w1


lonely_w1 ~ livealn_w1
lonely_w1 ~ hadsd_w1
lonely_w1 ~ hadsa_w1


'

sem_model_w1 <- sem(semmodel_w1, LBC1936, 
                    missing = 'FIML')
summary(sem_model_w1, fit.measures = T, standardized =T, ci = T)




standardizedSolution(sem_model_w1, type = "std.all", se = TRUE, zstat = TRUE, 
                     pvalue = TRUE, ci = TRUE, level = 0.95, cov.std = TRUE, 
                     remove.eq = TRUE, remove.ineq = TRUE, remove.def = FALSE, 
                     partable = NULL, GLIST = NULL, est = NULL,
                     output = "data.frame")

