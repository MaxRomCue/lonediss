LBC1936 %>%
  describe %>%
  as.data.frame() %>%
  rownames_to_column(., var = "variable")%>%
  select(variable, n, mean, sd, skew, kurtosis)%>%
  kable(digits = 2)%>%
  kable_styling(full_width = F)

LBCtest %>%
  pivot_longer(2:11, names_to = "variable", values_to="score") %>%
  ggplot(aes(x=score))+
  geom_density()+
  facet_wrap(~variable)+
  theme_light()



#RMSEA, SRMR .05; TLI, CFI .95
#CFA

modelcholes <- '
cholesbio =~ bld_hdlchol_w1 + bld_triglyc_w1
choles =~ cholesbio + hichol_w1

choles ~ sex
choles ~ lonely_w1
choles ~ livealn_w1
choles ~ hiqual_w1
choles ~ phyactiv_w1
choles ~ hmsonum_w1
choles ~ hadsd_w1
choles ~ hadsa_w1




'
modelfitcholes <- cfa(modelcholes, LBC1936, std.lv = T, 
                      missing = 'FIML')
summary(modelfitcholes, fit = T, standardized = T)
modindices(modelfitcholes, sort = T)


modelbp <-'
dbp =~ dbp1sit_w1 + dbp2sit_w1 + dbp3sit_w1
sbp =~ sbp1sit_w1 + sbp2sit_w1 + sbp3sit_w1
bpbio =~ sbp + dbp
bp =~ bpbio + hibp_w1


bp ~ sex
bp ~ lonely_w1
bp ~ livealn_w1
bp ~ hiqual_w1
bp ~ phyactiv_w1
bp ~ hmsonum_w1
bp ~ hadsd_w1
bp ~ hadsa_w1


'
modelfitbp <- cfa(modelbp, LBC1936, std.lv = T,
                  missing = 'FIML')
summary(modelfitbp, fit = T, standardized = T)


modelcardio <-'
cardio =~ bld_crprot_w1 + cvdhist_w1  


cardio ~ sex
cardio ~ lonely_w1
cardio ~ livealn_w1
cardio ~ hiqual_w1
cardio ~ phyactiv_w1
cardio ~ hmsonum_w1
cardio ~ hadsd_w1
cardio ~ hadsa_w1



'
modelfitcardio <- cfa(modelcardio, LBC1936, std.lv = T,
                      missing = 'FIML')
summary(modelfitcardio, fit = T, standardized = T)


modeldiab <-'
diab =~ bld_hba1c_w1 + diab_w1

diab ~ sex
diab ~ lonely_w1
diab ~ livealn_w1
diab ~ hiqual_w1
diab ~ phyactiv_w1
diab ~ hmsonum_w1
diab ~ hadsd_w1
diab ~ hadsa_w1


'
modelfitdiab <- cfa(modeldiab, LBC1936, std.lv = T,
                    missing = 'FIML')
summary(modelfitdiab, fit = T, standardized = T)



#SEM 
sem_model1 <- '

cholesbio =~ bld_hdlchol_w1 + bld_triglyc_w1
choles =~ cholesbio + hichol_w1

dbp =~ dbp1sit_w1 + dbp2sit_w1 + dbp3sit_w1 
sbp =~ sbp1sit_w1 + sbp2sit_w1 + sbp3sit_w1 
bp =~ dbp + sbp

cardio =~ cvdhist_w1

diab =~ bld_hba1c_w1 + diab_w1


choles ~ lonely_w1
choles ~ sex


bp ~ lonely_w1
bp ~ sex


cardio ~ lonely_w1
cardio ~ sex

diab ~ lonely_w1
diab ~ sex




#biomarkers covariances
bld_triglyc_w1 ~~ bld_hba1c_w1
bld_triglyc_w1 ~~ dbp
bld_hdlchol_w1 ~~ bld_hba1c_w1
bld_hdlchol_w1 ~~ sbp
bld_hba1c_w1 ~~ dbp
bld_hba1c_w1 ~~ sbp

bld_triglyc_w1  ~~ sbp
bld_hdlchol_w1 ~~ dbp



#selfreport covariances
hichol_w1 ~~ diab_w1
'
sem_model_1 <- sem(sem_model1, LBC1936, 
                    missing = 'FIML')
summary(sem_model_1, fit.measures = T, standardized =T, ci = T)



